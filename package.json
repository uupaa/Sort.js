{
  "name":           "uupaa.sort.js",
  "version":        "0.8.3",
  "description":    "Sort.js is nat-sort and random-sort implement.",
  "url":            "https://github.com/uupaa/Sort.js",
  "keywords":       ["natsort", "sort", "array", "uupaa"],
  "repository": {
    "type":         "git",
    "url":          "https://github.com/uupaa/Sort.js.git"
  },
  "scripts": {
    "watch":        "node node_modules/uupaa.watch.js --verbose --action build ./lib/Sort.js",
    "build":        "node node_modules/uupaa.minify.js --keep --option 'formatting pretty_print' --output ./lib/Sort.min.js ./lib/Sort.js",
    "build-release":"node node_modules/uupaa.minify.js --module all --output ./lib/Sort.min.js",
    "test":         "NODE_ENV=production NODE_PATH=lib node --harmony test/index.node.js; open test/index.html",
    "lint":         "plato --title Sort.js --jshint .jshintrc -d lint/plato lib/Sort.js; open lint/plato/index.html"
  },
  "build": {
    "all": { "lib/Sort.js": "" },
    "node": { },
    "worker": { },
    "browser": { }
  },
  "dependencies": {
  },
  "devDependencies": {
    "uupaa.help.js":    "",
    "uupaa.test.js":    "",
    "uupaa.task.js":    "",
    "uupaa.minify.js":  ""
  },
  "lib":            "./lib/",
  "main":           "./index.js",
  "author":         "uupaa <uupaa.js@gmail.com>",
  "license":        "MIT",
  "contributors":   []
}


